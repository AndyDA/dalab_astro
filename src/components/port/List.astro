---
import Item from '~/components/port/ListItem.astro';
import type { Portfolio } from '~/types';

export interface Props {
  posts: Array<Portfolio>;
}

const { posts } = Astro.props;
---

<ul>
  {
    posts.map((post) => (
      <li class='mb-12 md:mb-20'>
        <Item post={post} />
      </li>
    ))
  }
</ul>
